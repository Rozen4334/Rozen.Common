<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<RootNamespace>Common.Terraria</RootNamespace>

	<AssemblyVersion>1.0.0</AssemblyVersion>
	<PackageVersion>1.0.0</PackageVersion>
	<FileVersion>1.0.0.0</FileVersion>
	<Version>1.0.0</Version>

	<Authors>Armano den Boef.</Authors>

	<PackageProjectUrl>https://github.com/Rozen4334/Rozen.Common</PackageProjectUrl>
	<RepositoryUrl>https://github.com/Rozen4334/Rozen.Common</RepositoryUrl>
	<RepositoryType>git</RepositoryType>

	<PackageReleaseNotes>Version 1.0; Made for net6.</PackageReleaseNotes>
	<PackageTags>Common, Framework, Extensions, Terraria</PackageTags>

	<Title>Rozen.Common.Terraria</Title>
	<Description>A library containing numerous features commonly used in my applications. This package features Terraria plugin dependencies.</Description>

	<NeutralLanguage>en-US</NeutralLanguage>

	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageId>Rozen.Common.Terraria</PackageId>

	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<Copyright>Armano den Boef.</Copyright>

	<PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
	  <None Include="..\..\README.md">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
	  </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CSF.NET.TShock" Version="1.2.0" />
  </ItemGroup>

</Project>
